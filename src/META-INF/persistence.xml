<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="Bangun_GOPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>org.bangungo.model.Pendaftaran</class>
    <class>org.bangungo.model.Transaksi</class>
    <class>org.bangungo.model.Material</class>
    <class>org.bangungo.model.Komentar</class>
    <class>org.bangungo.model.Coba</class>
    <class>org.bangungo.model.Admin</class>
    <class>org.bangungo.model.User</class>
    <class>org.bangungo.model.Login</class>
    <class>org.bangungo.model.Barang</class>
    <class>org.bangungo.model.Keluhan</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/bangungo?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value=""/>
    </properties>
  </persistence-unit>
</persistence>
